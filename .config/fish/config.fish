# rbenv
status --is-interactive; and source (rbenv init -|psub)

pyenv init - | source

source ~/.config/fish/completions/custom.fish
source "$HOME/.cargo/env.fish"

abbr -a -- gall 'git add --all'
abbr -a -- gb 'git branch'
abbr -a -- gcm 'git commit -m'
abbr -a -- tm 'teamocil --here'
abbr -a -- glg 'git log --decorate'
abbr -a -- be 'bundle exec'
abbr -a -- rtodo 'vim ~/Nextcloud/Other/todo-txt/railslove.todo.txt'
abbr -a -- ber 'bundle exec rspec'
abbr -a -- t todo-txt
abbr -a -- gcam 'git commit -am'
abbr -a -- rs 'rails s'
abbr -a -- ga 'git add'
abbr -a -- gclean 'git clean -fdi'
abbr -a -- tt 'todo-txt ls'
abbr -a -- rt 'todo-txt -d .todo/railslove.config'
abbr -a -- todo 'vim ~/Nextcloud/Other/todo-txt/todo.txt'
abbr -a -- gco 'git checkout'
abbr -a -- rtt 'todo-txt -d ~/.todo/railslove.config ls'
abbr -a -- gm 'git merge'
abbr -a -- gs 'git switch'
abbr -a -- gst 'git status'
abbr -a -- bern 'bundle exec rspec -n --no-profile'
abbr -a -- drr 'dr web rspec'
abbr -a -- drw 'dr web'
